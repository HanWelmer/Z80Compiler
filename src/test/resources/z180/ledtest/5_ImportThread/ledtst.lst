21DA CB 17               RL    A           ;16* 7=112 269   Q = Q * 2
21DC ED 6A               ADC   HL,HL       ;16*10=160 429   R = R * 2 + carry
21DE B7                  OR    A,A         ;16* 4= 64 493   if (R >= D) {
21DF ED 52               SBC   HL,DE       ;16*10=160 653
21E1 38 03               JR    C,div16_2   ;16* 8=128 781 16*6= 96 749   R = R - D
21E3 0C                  INC   C           ;              16*4= 64 813   Q++
21E4 18 01               JR    div16_3     ;              16*8=128 941
21E6             div16_2:
21E6 19                  ADD   HL,DE       ;16* 7=112 893  (compensate comparison)
21E7             div16_3:
