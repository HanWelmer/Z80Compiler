21CF             ;T = AC = dividend
21CF             ;D = DE = divisor
21CF             ;Q = AC = quotient = 0
21CF             ;R = HL = remainder = 0
21CF             ;invariante betrekking:
21CF             ; D/T\Q     
21CF             ;   R       
21CF             ; T = QD + R
21CF             ; T <= 2^N  
21CF             ;
21CF             ; D/T'.RT\Q'      
21CF             ;   R'             
21CF             ; RT <= 2^N        
21CF             ; 0<=k<=N          
21CF             ; RT = T % 2^k     
21CF             ; T' = (T-RT) / 2^k
21CF             ; Q' = T' / D      
21CF             ; R' = T' % D      
21CF             ;
21CF             ;for (i=16; i>0; i--) {
21CF             ;  T = T * 2 (remember MSB in carry)
21CF             ;  R = R * 2 + carry
21CF             ;  Q = Q * 2
21CF             ;  if (R >= D) {
21CF             ;    R = R - D;
21CF             ;    Q++;
21CF             ;  }
