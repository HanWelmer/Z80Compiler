/* Program to test branch instructions */
class TestBranches {
  write(9);
  int a = 1;
  if (a == 1) write(8);
  //commentaar.
  if (a != 0) write(7);
  if (a > 0) write(6);
  if (a >= 0) write(5);
  if (a >= 1) write(4);
  if (a < 2) write(3);
  if (a <= 2) write(2);
  if (a <= 1) write(1);
  write(0);
}