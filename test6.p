/*
 * A small program in the miniJava language.
 * Test 8-bit and 16-bit operations
 */
class TestComparison {
  write(257);
  int i = 256;
  write(i);
  byte b = 255;
  write(b);
  int j = 5;
  write(1002 + j);
  write(j + 1001);
  byte d = 1;
  byte e = 2;
  write(d + 4);
  write(2 + e);
  write(d + e);
  b = 0 + 2;
  write(b);
  byte c = 1;
  write(c);
  write(0);
}
